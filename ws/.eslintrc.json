{
	"root": true,
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-unused-vars": "off",
		"no-mixed-spaces-and-tabs": "off",
		"no-undef": "error"
	}
}
